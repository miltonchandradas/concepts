{
   "name": "newprocess",
   "version": "1.0.0",
   "description": "A simple CAP project.",
   "repository": "<Add your repository here>",
   "license": "UNLICENSED",
   "private": true,
   "dependencies": {
      "@milton/oldprocess": "../oldprocess",
      "@sap/cds": "^6.6.1",
      "express": "^4"
   },
   "devDependencies": {
      "sqlite3": "^5.0.2"
   },
   "scripts": {
      "start": "cds run"
   },
   "eslintConfig": {
      "extends": "eslint:recommended",
      "env": {
         "es2020": true,
         "node": true,
         "jest": true,
         "mocha": true
      },
      "globals": {
         "SELECT": true,
         "INSERT": true,
         "UPDATE": true,
         "DELETE": true,
         "CREATE": true,
         "DROP": true,
         "CDL": true,
         "CQL": true,
         "CXL": true,
         "cds": true
      },
      "rules": {
         "no-console": "off",
         "require-atomic-updates": "off"
      }
   },
   "cds": {
      "requires": {
         "DemoService": {
            "kind": "odata",
            "model": "../oldprocess/srv/demo-service"
         },
         "messaging": {
            "kind": "file-based-messaging"
         },
         "db": {
            "kind": "sql"
         }
      }
   }
}
